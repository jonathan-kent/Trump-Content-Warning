{
  "name": "Trump Content Warning",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.min.js", "content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": ["trump_angry.png", "trump_neutral.png", "trump_sad.png"]
}
